import wollok.game.*
import auto.*



program miPrimerWollokGame {
	game.title("Autitos chocadores")
	game.width(15)
	game.height(15)
	game.boardGround("empedrado.jpg")
	
	
	game.addVisualCharacter(auto)
	game.addVisual(auto2)
	
	game.whenCollideDo(auto, {auto2 =>
		auto2.subir()
		game.say(auto2, auto2.quejarse())
	})
	game.onTick(2500, "movimiento", {auto2.movete()})
	game.start()
	

}